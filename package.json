{
  "name": "vnts-ws-relay",
  "version": "1.0.0",
  "description": "VNT WebSocket Relay for Cloudflare Workers",
  "main": "src/worker.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --ip 0.0.0.0 --var VERSION:$(date +%s)",
    "start": "wrangler deploy --var VERSION:$(date +%s)",
    "deploy": "wrangler deploy --var VERSION:$(date +%s)",
    "tail": "wrangler tail"
  },
  "devDependencies": {
    "protobufjs": "^8.0.0",
    "wrangler": "^3.0.0"
  },
  "keywords": [
    "vnt",
    "websocket",
    "cloudflare-workers",
    "durable-objects",
    "vpn",
    "p2p"
  ],
